#!/home/nick/projects/python/taxi-app/env/bin/python3.8
# EASY-INSTALL-ENTRY-SCRIPT: 'green==3.0.0','console_scripts','green'
__requires__ = 'green==3.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('green==3.0.0', 'console_scripts', 'green')()
    )
